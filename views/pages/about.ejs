<%- contentFor('content') %>
<div class='container'>
	<div class='row'>
		<div class="col-sm-12 col-md-6">
			<h2>The Team</h2>
			
			<div class='row'>
				<% for (user of users) { %>
					<div class='col-sm-3'> 
						<h3><%= user.name %></h3>
						<p><%= user.email %></p>						
					</div>
				<% } %>
			</div>
		</div>
	</div>
</div>